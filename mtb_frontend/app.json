{
  "expo": {
    "name": "My Town Bus",
    "slug": "MyTownBus",
    "scheme": "TownBusScheme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#d3a180"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#d3a180"
      },
      "package": "com.anonymous.MyTownBus"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f1e8657b-f62e-4875-8a02-66335eb0c40e"
      }
    }
  }
}
